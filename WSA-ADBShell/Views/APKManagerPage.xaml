<Page
    x:Class="WSA_ADBShell.Views.APKManagerPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:WSA_ADBShell.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="clr-namespace:SimpleUI.Controls;assembly=SimpleUI"
    xmlns:viewmodels="clr-namespace:WSA_ADBShell.ViewModels"
    Title="APKManagerPage"
    d:DataContext="{d:DesignInstance Type=viewmodels:APKManagerViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">

    <Grid>
        <ui:Button
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Command="{Binding OpenApkInstallCommand}"
            Content="安装测试"
            Icon="Go" />
    </Grid>
</Page>
